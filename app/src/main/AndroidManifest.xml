<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.amarjeet.android.teckatproject">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:screenOrientation="portrait"
        android:supportsRtl="true"
        android:theme="@style/NoBar">
        <activity android:name=".web_tsip"></activity>
        <activity android:name=".web_contacts" />
        <activity android:name=".web_faq" />
        <activity android:name=".trainings_web" />
        <activity
            android:name=".PrivacyPolicy"
            android:screenOrientation="portrait"
            android:theme="@style/NoBar" />
        <activity
            android:name=".PaymentDetails"
            android:label="@string/dashboard"
            android:screenOrientation="portrait" />
        <activity
            android:name=".Login_ui"
            android:screenOrientation="portrait"
            android:theme="@style/NoBar" />
        <activity
            android:name=".MainActivity"
            android:screenOrientation="portrait"
            android:theme="@style/NoBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <service
            android:name=".services.MyFirebaseInstanceService"
            android:directBootAware="true"
            android:exported="false">
            <intent-filter>
                <action android:name="com.google.firebase.MESSAGING_EVENT" />
            </intent-filter>
        </service>
    </application>

</manifest>